    <param pos="0" name="hw.product" value="Adaptive Security Appliance"/>
    <param pos="0" name="hw.device" value="Firewall"/>
    <param pos="0" name="hw.cpe23" value="cpe:/h:cisco:adaptive_security_appliance:-"/>
  </fingerprint>

</fingerprints>